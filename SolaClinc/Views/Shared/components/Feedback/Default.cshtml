@model IEnumerable<Feedback> 


<!--Feedbacks Start -->
<div class="services">
	<div class="container">
		<div class="row">
			<div class="col text-center">
				<div class="section_title_container">

					<div class="section_title"><h2>Our Patients Feedbacks</h2></div>

				</div>
			</div>
		</div>
		<div class="row services_row">

			@foreach (var item in Model)
			{


				<div class="col-xl-4 col-md-6 service_col">
					<div class="service text-center">
						<div class="service">
							<div class="icon_container d-flex flex-column align-items-center justify-content-center ml-auto mr-auto">
								<div> <img style="border-radius: 50%;width:163px;height:163px" src="@Url.Content(item.Fedimg) "></div>
							</div>

							<div class="service_title">@item.Name</div>
							<br>
							<div class="section_subtitle">@item.Email</div>
							<br>
							<div class="section_subtitle">@item.Service </div>
							<div class="service_text">
								<p>@item.Description</p>
							</div>
						</div>
					</div>
				</div>
			}





		</div>
	</div>
</div>

<!--Feedbacks End-->
